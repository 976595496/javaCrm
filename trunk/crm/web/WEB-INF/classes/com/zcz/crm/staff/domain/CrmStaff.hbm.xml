<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zcz.crm.staff.domain.CrmStaff" table="crm_staff">
        <id name="staffId">
            <generator class="uuid"/>
        </id>
        <property name="staffName" />
        <property name="loginName" />
        <property name="loginPwd" />
        <property name="gender" />
        <property name="onDutyDate">
            <column name="onDutyDate" sql-type="datetime"/>
        </property>
        <many-to-one name="post" class="com.zcz.crm.post.domain.CrmPost" column="postId"/>
    </class>
</hibernate-mapping>